{
	"version": 3,
	"routes": [
		{
			"src": "/404",
			"headers": {
				"Location": "/404/"
			},
			"status": 308
		},
		{
			"src": "/novel/([^/]+?)/chapter/([^/]+?)",
			"headers": {
				"Location": "/novel/$1/chapter/$2/"
			},
			"status": 308
		},
		{
			"src": "/novel/([^/]+?)",
			"headers": {
				"Location": "/novel/$1/"
			},
			"status": 308
		},
		{
			"src": "/search",
			"headers": {
				"Location": "/search/"
			},
			"status": 308
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/novel\\/([^/]+?)\\/chapter\\/([^/]+?)\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/novel\\/([^/]+?)\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/search\\/$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}